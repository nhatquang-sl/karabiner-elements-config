{
    "title": "CtrlCommand",
    "rules": [
        {
            "description": "CtrlCommand: Control enables CtrlCommand mode.",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "left_control",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "set_variable": {
                                "name": "ctrlcommand_mode",
                                "value": 1
                            }
                        }
                    ],
                    "to_if_alone": [
                        {
                            "key_code": "left_control"
                        }
                    ],
                    "to_after_key_up": [
                        {
                            "set_variable": {
                                "name": "ctrlcommand_mode",
                                "value": 0
                            }
                        }
                    ]
                }
            ]
        },
