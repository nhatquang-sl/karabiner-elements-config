{
    "title": "SpaceFN Launch Apps",
    "rules": [
        {
            "description": "SpaceFN Launch Apps: Space+[1, 2, 3, 4, 5] to Launch VSCode, Chrome, Rider, Slack, Terminal",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "1"
                    },
                    "to": [
                        {
                            "shell_command": "open '/Applications/Visual Studio Code.app'"
                        }
                    ],
                    "conditions": [
                        {
                            "type": "variable_if",
                            "name": "spacefn_mode",
                            "value": 1
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "2"
                    },
                    "to": [
                        {
                            "shell_command": "open '/Applications/Google Chrome.app'"
                        }
                    ],
                    "conditions": [
                        {
                            "type": "variable_if",
                            "name": "spacefn_mode",
                            "value": 1
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "3"
                    },
                    "to": [
                        {
                            "shell_command": "open '/Applications/Rider.app'"
                        }
                    ],
                    "conditions": [
                        {
                            "type": "variable_if",
                            "name": "spacefn_mode",
                            "value": 1
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "4"
                    },
                    "to": [
                        {
                            "shell_command": "open '/Applications/Slack.app'"
                        }
                    ],
                    "conditions": [
                        {
                            "type": "variable_if",
                            "name": "spacefn_mode",
                            "value": 1
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "5"
                    },
                    "to": [
                        {
                            "shell_command": "open '/Applications/Utilities/Terminal.app'"
                        }
                    ],
                    "conditions": [
                        {
                            "type": "variable_if",
                            "name": "spacefn_mode",
                            "value": 1
                        }
                    ]
                }
            ]
        }
    ]
}