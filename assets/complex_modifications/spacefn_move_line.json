{
    "title": "SpaceFN Move Line",
    "rules": [
        {
            "description": "SpaceFN Move Line: Space+[w, s] to move line Up/Down",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "w"
                    },
                    "to": [
                        {
                            "key_code": "up_arrow",
                            "modifiers": [
                                "option"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "variable_if",
                            "name": "spacefn_mode",
                            "value": 1
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "s"
                    },
                    "to": [
                        {
                            "key_code": "down_arrow",
                            "modifiers": [
                                "option"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "variable_if",
                            "name": "spacefn_mode",
                            "value": 1
                        }
                    ]
                }
            ]
        }
    ]
}